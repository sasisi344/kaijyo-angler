baseURL = "https://example.org/"

# theme
theme = "hugo-theme-bootstrap"

# website title
title = "Kaijyo Fishing"

# determines default content language ["en", "zh-cn", "fr", "pl", "ja", ...]
defaultContentLanguage = "ja"
# language code ["en", "zh-CN", "fr", "pl", "ja", ...]
languageCode = "ja"
# language name ["English", "简体中文", "Français", "Polski", "日本語", ...]
languageName = "日本語"
# whether to include Chinese/Japanese/Korean
hasCJKLanguage = true

# copyright description used only for seo schema
copyright = ""

# whether to use robots.txt
enableRobotsTXT = true
# whether to use git commit log
enableGitInfo = true
# whether to use emoji code
enableEmoji = true

# ignore some build errors
ignoreErrors = ["error-remote-getjson", "error-missing-instagram-accesstoken"]

[build]
  writeStats = true

[params]
  [params.social]
    github = "sasisi344/kaijyo-fishing"
  [params.author]
    name = "Kaijyo Fishing"

# Pagination config
[pagination]
  disableAliases = false
  pagerSize = 10
  path = "page"

# Multilingual
[languages]
  [languages.ja]
    weight = 1
    languageCode = "ja"
    languageName = "日本語"
    hasCJKLanguage = true
    title = "Kaijyo Fishing"
    description = "海釣り施設と海上釣り堀の情報サイト"
    [languages.ja.menu]
      [[languages.ja.menu.main]]
        weight = 1
        identifier = "posts"
        name = "記事"
        url = "/posts/"
        title = ""
      [[languages.ja.menu.main]]
        weight = 2
        identifier = "categories"
        name = "カテゴリー"
        url = "/categories/"
        title = ""
      [[languages.ja.menu.main]]
        weight = 3
        identifier = "category-eastjapan"
        name = "東日本"
        url = "/categories/東日本/"
        title = "東日本地方の海上釣り堀・海釣り施設"
        parent = "categories"
      [[languages.ja.menu.main]]
        weight = 4
        identifier = "category-chubu"
        name = "中部"
        url = "/categories/中部/"
        title = "中部地方の海上釣り堀・海釣り施設"
        parent = "categories"
      [[languages.ja.menu.main]]
        weight = 5
        identifier = "category-westjapan"
