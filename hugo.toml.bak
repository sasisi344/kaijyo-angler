        name = "タグ"
        url = "/tags/"
        title = ""
    [languages.ja.params]
      defaultTheme = "light"
      gitRepo = "https://github.com/sasisi344/kaijyo-fishing"
      fingerprint = ""
      dateFormat = "2006-01-02"
      title = "Kaijyo Fishing"
      description = "海釣り施設と海上釣り堀の情報サイト"
      images = ["/logo.png"]
      [languages.ja.params.author]
        name = "Kaijyo Fishing"
        email = ""
        link = ""
      [languages.ja.params.header]
        desktopMode = "fixed"
        mobileMode = "auto"
        [languages.ja.params.header.title]
          logo = ""
          name = "Kaijyo Fishing"
          pre = ""
          post = ""
          typeit = false
      [languages.ja.params.footer]
        enable = true
        custom = ""
        hugo = true
        copyright = true
        author = true
        since = 2025
        icp = ""
        license = ""
      [languages.ja.params.section]
